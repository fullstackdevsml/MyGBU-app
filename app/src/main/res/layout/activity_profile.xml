<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ProfileActivity">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/purple_700"
        android:elevation="4dp"
        android:layout_marginTop="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/profile"
            android:textColor="@android:color/white"
            android:textSize="30sp"
            android:textStyle="bold"> </TextView>
    </androidx.appcompat.widget.Toolbar>


    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:baselineAligned="false"
        android:orientation="horizontal"
        app:layout_constraintVertical_bias="0.2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="0dp"
            android:layout_weight="1"
            android:background="@android:color/white"
            android:gravity="center_horizontal"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <ImageView
                android:layout_width="175dp"
                android:layout_height="138dp"
                android:src="@drawable/crcle"
                tools:ignore="ContentDescription"> </ImageView>

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                android:layout_marginStart="10dp"
                android:text="@string/name"
                android:textStyle="bold" />
            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="30dp"
                android:layout_height="39dp"
                android:src="@drawable/user2"
                android:layout_marginStart="-160dp"
                android:layout_marginBottom="-38dp"
                tools:ignore="ContentDescription" />

            <EditText
                android:id="@+id/editTextTextPersonName2"
                android:layout_height="42dp"
                android:layout_width="300dp"
                android:layout_marginStart="20dp"
                android:background="@drawable/edit_text_bg"
                android:ems="10"
                android:inputType="textPersonName"
                android:paddingStart="20dp"
                android:paddingTop="10dp"
                android:paddingEnd="0dp"
                android:paddingBottom="10dp"
                android:text="Oshi Raghav"
                tools:ignore="Autofill,LabelFor" />

            <TextView
                android:id="@+id/About"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="10dp"
                android:text="@string/about"
                android:textStyle="bold"
                android:layout_marginStart="10dp"/>
            <ImageView
                android:id="@+id/aboutimg"
                android:layout_width="30dp"
                android:layout_height="39dp"
                android:src="@drawable/info_button"
                android:layout_marginStart="-160dp"
                android:layout_marginBottom="-38dp"
                tools:ignore="ContentDescription" />

            <EditText
                android:id="@+id/editAbout"
                android:layout_width="300dp"
                android:layout_marginStart="20dp"
                android:layout_height="42dp"
                android:background="@drawable/edit_text_bg"
                android:ems="10"
                android:inputType="textMultiLine"
                android:paddingStart="20dp"
                android:paddingTop="10dp"
                android:paddingEnd="0dp"
                android:paddingBottom="10dp"
                android:text="@string/hey_there"
                tools:ignore="Autofill,LabelFor" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/phone"
                android:textStyle="bold"
                android:layout_marginStart="10dp"/>

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="37dp"
                android:layout_height="49dp"
                android:layout_marginStart="-160dp"
                android:layout_marginBottom="-49dp"
                android:src="@drawable/phone_call"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/linearLayout"
                app:layout_constraintHorizontal_bias=".1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                app:layout_constraintVertical_bias="0.08"
                tools:ignore="ContentDescription" />

            <EditText
                android:id="@+id/editTextPhone4"
                android:layout_width="300dp"
                android:layout_marginStart="20dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/edit_text_bg"
                android:ems="10"
                android:importantForAutofill="no"
                android:inputType="phone"
                android:paddingStart="20dp"
                android:paddingTop="10dp"
                android:paddingEnd="20dp"
                android:paddingBottom="10dp"
                android:text="+918127******"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/roll_no"
                android:textStyle="bold"
                android:layout_marginStart="10dp"/>
            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="37dp"
                android:layout_height="49dp"
                android:layout_marginStart="-160dp"
                android:layout_marginBottom="-45dp"
                android:src="@drawable/driver_license"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/linearLayout"
                app:layout_constraintHorizontal_bias=".09"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView12"
                app:layout_constraintVertical_bias="0.08"
                tools:ignore="ContentDescription" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView"
                android:layout_width="300dp"
                android:layout_marginStart="20dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/edit_text_bg"
                android:paddingStart="20dp"
                android:paddingTop="10dp"
                android:paddingEnd="20dp"
                android:paddingBottom="10dp"
                android:text="215/UCS/004"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.296"
                app:layout_constraintWidth_percent=".8"
                tools:ignore="LabelFor" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="23dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:text="@string/school_and_branch"
                android:textStyle="bold" />
            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="37dp"
                android:layout_height="49dp"
                android:layout_marginStart="-160dp"
                android:layout_marginBottom="-48dp"
                android:src="@drawable/schools"
                android:textAlignment="center"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/linearLayout"
                app:layout_constraintHorizontal_bias=".091"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView13"
                app:layout_constraintVertical_bias="0.10"
                tools:ignore="ContentDescription" />

            <AutoCompleteTextView
                android:id="@+id/autoCompleteTextView2"
                android:layout_width="300dp"
                android:layout_marginStart="20dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/edit_text_bg"
                android:paddingStart="20dp"
                android:paddingTop="10dp"
                android:paddingEnd="20dp"
                android:paddingBottom="10dp"
                android:text="SoICT"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.296"
                app:layout_constraintWidth_percent=".8"
                tools:ignore="LabelFor" />

            <Button
                android:id="@+id/bsaveprofile"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginStart="10dp"
                android:background="@drawable/shape5_form"
                android:text="@string/edit_profile" />


        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>